#include"bits/stdc++.h"
#include<iostream>
 
using namespace std;
 
int main() {
 
    string str;
    int n,a=0,d=0;
    cin>>n>>str;
    sort(str.begin(),str.end());
    for(int i=0;i<str.size();i++){
        if(str[i]=='A')
          a++;
        else
         d++;
    }
    if(a>d)
        cout<<"Anton"<<endl;
    else if(d>a)
        cout<<"Danik"<<endl;
    else
        cout<<"Friendship"<<endl;
    return 0;
}
